{
  "manifest_version": 3,
  "name": "URL Saver",
  "version": "1.0",
  "description": "Save the current URL with a custom name",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "bookmark.png",
      "48": "bookmark.png",
      "128": "bookmark.png"
    }
  }
}
